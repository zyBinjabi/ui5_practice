<core:FragmentDefinition
    xmlns="sap.tnt"
    xmlns:core="sap.ui.core"
>
    <NavigationListItem
        id="NavigationListItem_id"
        text="{navList>title}"
        icon="{navList>icon}"
        enabled="{navList>enabled}"
        expanded="{navList>expanded}"
        items="{
			path: 'items',
			templateShareable: true
		}"
        key="{navList>key}"
    >
        <NavigationListItem
            id="NavigationListItem_nested_id"
            text="{navList>title}"
            key="{navList>key}"
            enabled="{navList>enabled}"
        />
    </NavigationListItem>
</core:FragmentDefinition>
